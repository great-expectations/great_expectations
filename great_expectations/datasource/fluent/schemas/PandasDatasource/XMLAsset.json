            "properties": {
                "column_name": {
                    "title": "Column Name",
                    "type": "string"
                },
                "sort_ascending": {
                    "title": "Sort Ascending",
                    "default": true,
                    "type": "boolean"
                },
                "method_name": {
                    "title": "Method Name",
