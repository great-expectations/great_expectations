{
    "title": "ExcelAsset",
    "description": "Read an Excel file into a pandas DataFrame.\n\nSupports `xls`, `xlsx`, `xlsm`, `xlsb`, `odf`, `ods` and `odt` file extensions\nread from a local filesystem or URL. Supports an option to read\na single sheet or a list of sheets.",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string"
        },
        "type": {
            "title": "Type",
            "default": "excel",
            "enum": [
                "excel"
            ],
            "type": "string"
        },
        "id": {
            "title": "Id",
            "description": "DataAsset id",
            "type": "string",
            "format": "uuid"
        },
        "order_by": {
            "title": "Order By",
            "type": "array",
            "items": {
                "$ref": "#/definitions/Sorter"
            }
        },
        "batch_metadata": {
            "title": "Batch Metadata",
            "type": "object"
        },
        "io": {
            "title": "Io",
            "anyOf": [
                {
                    "type": "string",
                    "format": "file-path"
                },
                {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 65536,
                    "format": "uri"
                },
                {}
            ]
        },
        "sheet_name": {
            "title": "Sheet Name",
            "default": 0
        },
        "header": {
            "title": "Header",
            "default": 0
        },
        "names": {
            "title": "Names"
        },
        "index_col": {
            "title": "Index Col"
        },
        "usecols": {
            "title": "Usecols",
            "anyOf": [
                {
                    "type": "integer"
                },
                {
                    "type": "string"
                },
                {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                }
            ]
        },
        "squeeze": {
            "title": "Squeeze",
            "default": false
        },
        "dtype": {
            "title": "Dtype",
            "type": "object"
        },
        "engine": {
            "title": "Engine"
        },
        "converters": {
            "title": "Converters"
        },
        "true_values": {
            "title": "True Values"
        },
        "false_values": {
            "title": "False Values"
        },
        "skiprows": {
            "title": "Skiprows",
            "anyOf": [
                {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                {
                    "type": "integer"
                }
            ]
        },
        "nrows": {
            "title": "Nrows"
        },
        "na_values": {
            "title": "Na Values"
        },
        "keep_default_na": {
            "title": "Keep Default Na",
            "default": true
        },
        "na_filter": {
            "title": "Na Filter",
            "default": true
        },
        "verbose": {
            "title": "Verbose",
            "default": false
        },
        "parse_dates": {
            "title": "Parse Dates",
            "default": false
        },
        "date_parser": {
            "title": "Date Parser"
        },
        "thousands": {
            "title": "Thousands"
        },
        "comment": {
            "title": "Comment"
        },
        "skipfooter": {
            "title": "Skipfooter",
            "default": 0
        },
        "convert_float": {
            "title": "Convert Float"
        },
        "mangle_dupe_cols": {
            "title": "Mangle Dupe Cols",
            "default": true
        },
        "storage_options": {
            "title": "Storage Options",
            "type": "object"
        }
    },
    "required": [
        "name",
        "io"
    ],
    "additionalProperties": false,
    "definitions": {
        "Sorter": {
            "title": "Sorter",
            "type": "object",
            "properties": {
                "key": {
                    "title": "Key",
                    "type": "string"
                },
                "reverse": {
                    "title": "Reverse",
                    "default": false,
                    "type": "boolean"
                }
            },
            "required": [
                "key"
            ]
        }
    }
}
