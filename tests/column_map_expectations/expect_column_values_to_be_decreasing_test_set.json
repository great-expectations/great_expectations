{
  "expectation_type" : "expect_column_values_to_be_decreasing",
  "datasets" : [{
    "data" : {
      "w" : [1, 2, 3, 7, 6, 5, 4, 3, 2, 1],
      "x" : [null, null, 10, 9, 8, 7, null, 6, 5, 4],
      "y" : [1, 1, 1, 2, 2, 2, 3, 3, 3, 4],
      "z" : ["12/1/2016", "11/2/2016", "10/2/2016", "10/2/2016", "8/1/2016", null, null, null, null, null]
    },
    "tests" : [{
      "title" : "Positive test with mostly",
      "in": {
        "column": "w",
        "mostly": 0.6
      },
      "out": {
        "unexpected_list": [2,3,7],
        "unexpected_index_list": [1,2,3],
        "success": true
      }
    },{
      "title" : "Basic negative test",
      "in": {
        "column": "y"
      },
      "out": {
        "unexpected_list": [2,3,4],
        "unexpected_index_list": [3,6,9],
        "success": false
      }
    },{
      "title" : "Basic negative test with strictly",
      "in": {
        "column": "y",
        "strictly": true
      },
      "out": {
        "unexpected_list": [1,1,2,2,2,3,3,3,4],
        "unexpected_index_list": [1,2,3,4,5,6,7,8,9],
        "success": false
      }
    },{
      "title" : "Basic positive test",
      "in": {
        "column": "x"
      },
      "out": {
        "unexpected_list": [],
        "unexpected_index_list": [],
        "success": true
      }
    },{
      "title" : "Positive test with parse_strings_as_datetimes",
      "in": {
        "column": "z",
        "parse_strings_as_datetimes" : true
      },
      "out": {
        "unexpected_list": [],
        "unexpected_index_list": [],
        "success": true
      }
    },{
      "title" : "Negative test with parse_strings_as_datetimes",
      "in": {
        "column": "z",
        "parse_strings_as_datetimes" : true,
        "strictly": true
      },
      "out": {
        "unexpected_list": ["10/2/2016"],
        "unexpected_index_list": [3],
        "success": false
      }
    }]
  }]
}