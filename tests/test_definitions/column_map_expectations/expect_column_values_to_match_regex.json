{
  "expectation_type" : "expect_column_values_to_match_regex",
  "datasets" : [{
    "data" : {
      "a" : ["aaa", "abb", "acc", "add", "bee"],
      "b" : ["aaa", "abb", "acc", "bdd", null],
      "c" : [ null,  null,  null,  null, null],
      "column_name with space": ["aaa", "abb", "acc", "add", "bee"]
    },
    "schemas": {
      "spark": {
        "a": "StringType",
        "b": "StringType",
        "c": "StringType",
        "column_name with space": "StringType"
      }
    },
    "tests": [{
      "title": "Negative test, insufficient Mostly + one non-matching value",
      "exact_match_out" : false,
      "in":{
        "column": "a",
        "regex": "^a",
        "mostly": 0.9
      },
      "out":{
        "success":false,
        "unexpected_index_list": [4],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test, exact mostly w/ one non-matching value",
      "exact_match_out" : false,
      "in":{
        "column": "a",
        "regex": "^a",
        "mostly": 0.8
      },
      "out":{
        "success":true,
        "unexpected_index_list": [4],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite"]
      },
      {
      "title": "Positive_test_column_name_has_space",
      "exact_match_out" : false,
      "in":{
        "column": "column_name with space",
        "regex": "^a",
        "mostly": 0.8
      },
      "out":{
        "success":true,
        "unexpected_index_list": [4],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite"]
      },
      {
      "title": "Positive test, sufficient mostly w/ one non-matching value",
      "exact_match_out" : false,
      "in":{
        "column": "a",
        "regex": "^a",
        "mostly": 0.7
      },
      "out":{
        "success":true,
        "unexpected_index_list": [4],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Negative test, one missing value and insufficent mostly",
      "exact_match_out" : false,
      "in":{
        "column": "b",
        "regex": "^a",
        "mostly": 0.8
      },
      "out":{
        "success":false,
        "unexpected_index_list": [3],
        "unexpected_list": ["bdd"]
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test, one missing value and exact mostly",
      "exact_match_out" : false,
      "in":{
        "column": "b",
        "regex": "^a",
        "mostly": 0.75
      },
      "out":{
        "success":true,
        "unexpected_index_list": [3],
        "unexpected_list": ["bdd"]
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test, one missing value and sufficent mostly",
      "exact_match_out" : false,
      "in":{
        "column": "b",
        "regex": "^a",
        "mostly": 0.7
      },
      "out":{
        "success":true,
        "unexpected_index_list": [3],
        "unexpected_list": ["bdd"]
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test. all missing values",
      "exact_match_out" : false,
      "in":{
        "column": "c",
        "regex": "^a"
      },
      "out":{
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test. all missing values, mostly",
      "exact_match_out" : false,
      "in":{
        "column": "c",
        "regex": "^a",
        "mostly": 0.2
      },
      "out":{
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test, Empty regex",
      "exact_match_out" : false,
      "in":{
        "column": "b",
        "regex": ""
      },
      "out":{
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test, more complicated regex pattern",
      "exact_match_out" : false,
      "in":{
        "column": "b",
        "regex": "[ab]{1}[a-d]{2}"
      },
      "out":{
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite"]
    },{
      "title": "Positive test, match characters not at the beginning of string",
      "exact_match_out" : false,
      "in":{
        "column": "a",
        "regex": "b",
        "mostly": 0.4
      },
      "out":{
        "success": true,
        "unexpected_index_list": [0, 2, 3],
        "unexpected_list": ["aaa", "acc", "add"]
      },
      "suppress_test_for": ["sqlite"]
    }]
  }]
}
